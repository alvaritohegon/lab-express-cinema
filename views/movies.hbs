<h1>Ironhack Cinema</h1>
<h2>Click on the movies to check showtimes!</h2>

    {{#each this.allMovies}}
         <div class="movie"> 
        <img src="{{this.image}}" alt="image_movie">
        <div>
            <p>{{this.title}}</p>
            <a href="/movie/{{this._id}}">See more</a>
        </div>
    </div>
    {{/each}}

</div>